<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<test name="smokesanity">
		<groups>
			<run>
				<include name="smoke" />
				<include name="sanity" />
			</run>
		</groups>
		<classes>
			<class name="week5day2_TestNG_Project.Lead_CreateLead" />
			<class name="week5day2_TestNG_Project.Lead_EditLead" />
			<class name="week5day2_TestNG_Project.Lead_MergeLead" />
			<class name="week5day2_TestNG_Project.Lead_DeleteLead" />
		</classes>
	</test> <!-- Test -->
	<test name="onlysmoke">
		<groups>
			<run>
				<include name="smoke" />
				<exclude name="sanity" />
			</run>
		</groups>
		<classes>
			<class name="week5day2_TestNG_Project.Lead_CreateLead" />
			<class name="week5day2_TestNG_Project.Lead_EditLead" />
			<class name="week5day2_TestNG_Project.Lead_MergeLead" />
			<class name="week5day2_TestNG_Project.Lead_DeleteLead" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
